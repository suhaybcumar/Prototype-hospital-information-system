@model PHIMS.Models.Result

@{
    ViewData["Title"] = "Create";
    Layout = null;
}

<div class="row">
    <div class="col">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="result" class="control-label"></label>
                <input asp-for="result" class="form-control" />
                <span asp-validation-for="result" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Advice" class="control-label"></label>
                <input asp-for="Advice" class="form-control" />
                <span asp-validation-for="Advice" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Pateint_ID" class="control-label"></label>
                <select asp-for="Pateint_ID" class ="form-control" asp-items="ViewBag.Pateint_ID"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary btn-block" />
            </div>
        </form>
    </div>
</div>

    
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
